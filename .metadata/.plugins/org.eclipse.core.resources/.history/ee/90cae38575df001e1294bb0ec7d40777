package time;
import java.util.Iterator;

public class TimeArray {
	public static void main(String[] args) {
		time t1=new time();
		t1.inputTime();
		
		time t2=new time(5,35,35);
		t2.setM(55);
		
		time t3=new time(5500);
		
		time tArray[]=new time[6]; // array declarations
		
		//objects creation for array
		tArray[0]=new time();//no argument const
		tArray[1]=new time(4,55,55);//three argument const
		
		for (int i = 2; i < tArray.length; i++) {
			tArray[i]=new time();
			tArray[i].inputTime();
		}
		
	}
}
